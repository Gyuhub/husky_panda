<?xml version="1.0" encoding="UTF-8"?>
<launch>
	<!-- Experiment settings -->
	<arg name="experiment_name" default="husky_panda_manipulation"/>

	<!-- Launch state observer -->
	<include file="$(find husky_panda_manipulation_bamboo)/launch/observer.launch">
	</include>

	<!-- Launch robot state controller -->
	<include file="$(find husky_panda_manipulation_bamboo)/launch/robot.launch">
		<arg name="object"  value="new_shelf"/>
		<arg name="experiment_name" value="$(arg experiment_name)"/>
	</include>

</launch>
